{
  "name": "geomaps-backend",
  "version": "1.0.0",
  "description": "Node.js backend server for South African membership management system",
  "main": "dist/app.js",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "nodemon src/app.ts",
    "build": "tsc && npm run copy-assets",
    "copy-assets": "node -e \"const fs = require('fs'); const path = require('path'); const src = path.join(__dirname, 'src', 'assets'); const dest = path.join(__dirname, 'dist', 'assets'); if (fs.existsSync(src)) { fs.cpSync(src, dest, { recursive: true }); console.log('✅ Assets copied to dist/assets'); } else { console.log('⚠️  No assets directory found'); }\"",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:api": "node test-api.js",
    "test:email": "node ../test/email/email-templates-test.js",
    "install:setup": "node install.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "setup": "npm install && npm run build && node install.js",
    "migrate": "node run-migrations.js run",
    "migrate:status": "node run-migrations.js status",
    "migrate:file-processing": "node run-file-processing-migration.js",
    "test:file-processing": "node test-file-processing.js",
    "setup:file-processing": "npm run migrate:file-processing && node -e \"const fs = require('fs'); const path = require('path'); ['uploads', 'uploads/excel-processing', 'uploads/excel-processing/processed'].forEach(dir => { const fullPath = path.join(__dirname, dir); if (!fs.existsSync(fullPath)) { fs.mkdirSync(fullPath, { recursive: true }); console.log('Created directory:', fullPath); } });\"",
    "generate-reports": "ts-node scripts/generate-reports.ts"
  },
  "keywords": [
    "nodejs",
    "express",
    "mysql",
    "membership",
    "south-africa",
    "api"
  ],
  "author": "GEOMAPS Team",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "@types/archiver": "^6.0.3",
    "@types/bull": "^3.15.9",
    "@types/node-cron": "^3.0.11",
    "@types/pdfkit": "^0.17.3",
    "@types/pg": "^8.15.5",
    "@types/qrcode": "^1.5.5",
    "@types/redis": "^4.0.10",
    "@types/speakeasy": "^2.0.10",
    "archiver": "^7.0.1",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.16.5",
    "chart.js": "^4.5.0",
    "chartjs-node-canvas": "^5.0.0",
    "check-disk-space": "^3.4.0",
    "chokidar": "^3.5.3",
    "cloudscraper": "^4.6.0",
    "compression": "^1.7.4",
    "connect-redis": "^9.0.0",
    "cors": "^2.8.5",
    "docx": "^9.5.1",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.5.1",
    "express-session": "^1.18.2",
    "express-slow-down": "^3.0.0",
    "helmet": "^7.2.0",
    "html-pdf-node": "^1.0.8",
    "ioredis": "^5.7.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "libreoffice-convert": "^1.7.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.5",
    "node-cron": "^4.2.1",
    "nodemailer": "^6.9.8",
    "pdfkit": "^0.17.2",
    "pg": "^8.16.3",
    "prisma": "^6.16.2",
    "qrcode": "^1.5.4",
    "redis": "^5.8.2",
    "smpp": "^0.6.0-rc.4",
    "socket.io": "^4.7.5",
    "speakeasy": "^2.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/chokidar": "^2.1.3",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/form-data": "^2.2.1",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.19.11",
    "@types/nodemailer": "^6.4.14",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "form-data": "^4.0.5",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
